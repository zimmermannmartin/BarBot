<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/barBot_setting"
    tools:context="at.barbot.barbot.BarbotSettingFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="191dp"
            android:layout_marginStart="191dp"
            android:layout_marginTop="243dp"
            android:id="@+id/textView3"
            tools:text="@string/Slave1"
            android:textSize="24sp"
            android:textStyle="normal|bold"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageView
            app:srcCompat="@color/colorPrimary"
            android:layout_marginLeft="167dp"
            android:layout_marginStart="167dp"
            android:layout_marginBottom="58dp"
            android:id="@+id/imageView3"
            android:layout_height="150dp"
            android:layout_width="200dp"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ExpandableListView
            android:layout_height="100dp"
            android:layout_marginTop="18dp"
            android:id="@+id/expandableListView"
            android:layout_below="@+id/textView3"
            android:layout_alignRight="@+id/imageView3"
            android:layout_alignEnd="@+id/imageView3"
            android:layout_width="150dp" />

        <TextView
            android:text="@string/Zutat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/expandableListView"
            android:id="@+id/textView10"
            android:textSize="18sp"
            android:layout_alignLeft="@+id/textView3"
            android:layout_alignStart="@+id/textView3" />

        <Button
            android:text="@string/Speichern"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/expandableListView"
            android:layout_alignLeft="@+id/expandableListView"
            android:layout_alignStart="@+id/expandableListView"
            android:layout_marginTop="540dp"
            android:id="@+id/button2"
            android:layout_marginLeft="220dp" />

        <EditText
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/Fuellstand"
            android:ems="10"
            android:layout_marginTop="410dp"
            android:id="@+id/editText3"
            android:textAlignment="center"
            android:layout_below="@+id/expandableListView"
            android:layout_alignRight="@+id/expandableListView"
            android:layout_alignEnd="@+id/expandableListView"
            android:layout_marginLeft="185dp"
            android:layout_width="175dp" />

        <ProgressBar
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText3"
            android:id="@+id/progressBar"
            android:layout_alignRight="@+id/expandableListView"
            android:layout_alignEnd="@+id/expandableListView"
            android:layout_alignLeft="@+id/imageView3"
            android:layout_alignStart="@+id/imageView3" />

    </RelativeLayout>
</FrameLayout>
